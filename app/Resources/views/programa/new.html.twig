{% extends 'base.html.twig' %}
{% form_theme form 'form/itemForm.html.twig' %}

{% block titulo %}
    Nuevo Programa Académico
{% endblock %}

{% block contenido %}
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header mt-4 mb-3">Nuevo Programa Académico</h1> 
        </div>
    </div>
    <div class="row">
        <div class="col-md-6">
            {{ form_start(form) }}
                {{ form_widget(form) }}
                <input type="submit" class="btn btn-success" value="Guardar" />
                <a class="btn btn-warning" href="{{ path('programa_index') }}">Cancelar</a>
            {{ form_end(form) }}
        </div>
    </div>
</div>
{% endblock %}
