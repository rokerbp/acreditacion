{% extends 'base.html.twig' %}

{% block titulo %}
    Editar Caracteristicas
{% endblock %}

{% block contenido %}
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Editar Caracteristicas</h1>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">

            {{ form_start(edit_form) }}
                {{ form_widget(edit_form) }}
                <input type="submit" value="Edit" />
            {{ form_end(edit_form) }}

            <ul>
                <li>
                    <a href="{{ path('caracteristica_model_index') }}">Back to the list</a>
                </li>
                <li>
                    {{ form_start(delete_form) }}
                        <input type="submit" value="Delete">
                    {{ form_end(delete_form) }}
                </li>
            </ul>

        </div>
    </div>
</div>
{% endblock %}
