{% extends 'base.html.twig' %}

{% block titulo %}
    Nueva Caracteristicas
{% endblock %}

{% block contenido %}
<div id="page-wrapper">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">Crear Caracteristicas</h1>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">

            {{ form_start(form) }}
                {{ form_widget(form) }}
                <input class="btn btn-success" type="submit" value="Crear" />
            {{ form_end(form) }}
            <a class="btn btn-primary" href="{{ path('caracteristica_model_index') }}">Atras</a>
        </div>
    </div>
</div>
{% endblock %}
