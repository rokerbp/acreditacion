<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Estudiante" table="estudiante">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="identificacion" type="string" column="identificacion" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="anioIngreso" type="datetime" column="anioIngreso" nullable="false"/>
    <field name="estado" type="string" column="estado" length="16" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="anioGraduacion" type="datetime" column="anioGraduacion" nullable="true">
      <options>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="ciudadOrigen" type="string" column="ciudadOrigen" length="128" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="correo" type="string" column="correo" length="128" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="telefono" type="string" column="telefono" length="16" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="ciudadActual" type="string" column="ciudadActual" length="128" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
